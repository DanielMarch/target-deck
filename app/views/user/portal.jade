extends ../shared/template

block content
  h1= title
  a(href='/obj/new') New Objective

  table
    thead
      tr
        th
          | Objective Photo
        th
          | Priority
        th
          | Operation
        th
          | Objective Name
        th
          | Status
        th
          | Edit/Delete
    tbody
      each obj in objs
        - var opIndex = _.findIndex(ops,{'_id': obj.opId})
        tr
          td
            each pic in obj.profilePic
              .photo(style="background-image:url('#{pic.file}')")
          td= obj.priority
          td= ops[opIndex].name
          td= obj.objname
          td= obj.status
          //- td
          //-   form(method='POST', action='/users/#{user._id}/delete')
          //-     button X
